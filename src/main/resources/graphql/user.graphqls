type UserQuery {
    userLogin(user: UserRequest!): UserResponse!
}

type UserMutation {
    newUser(user: UserRequest!): UserResponse
    updateUser(userId: ID!, user: UserRequest!): UserResponse
    deleteUser(userId: ID!): String
}

input UserRequest {
    first_name: String!
    last_name: String!
    email: String!
    password: String!
}

type UserResponse {
    id: ID!
    first_name: String!
    last_name: String!
    email: String!
    favorites: [BusinessResponse]
}