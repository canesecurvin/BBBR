extend type QueryResolver {
    user(email: String!): User!
}

extend type MutationResolver {
    newUser(user: UserInput!): User
}

input UserInput {
    first_name: String!
    last_name: String!
    email: String!
    password: String!
}

type User {
    id: ID!
    first_name: String!
    last_name: String!
    email: String!
    password: String!
    favorite_business: [Business]
}